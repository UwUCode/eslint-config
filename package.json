{
  "name": "@uwu-codes/eslint-config",
  "version": "1.1.28",
  "description": "My eslint config for my projects.",
  "main": "ts.json",
  "files": [
    "ts.json",
    "esm.json"
  ],
  "exports": {
    ".": {
      "import": "./esm.json",
      "default": "./ts.json"
    },
    "./esm": "./esm.json",
    "./ts": "./ts.json"
  },
  "scripts": {
    "lint": "eslint . --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UwUCode/eslint-config.git"
  },
  "author": "Donovan_DMC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UwUCode/eslint-config/issues"
  },
  "homepage": "https://github.com/UwUCode/eslint-config#readme",
  "engines": {
    "node": ">=18"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@uwu-codes/tsconfig": "^1.0.14",
    "eslint": "^9.1.0",
    "eslint-plugin-import": "npm:eslint-plugin-i@^2.29.1",
    "eslint-plugin-import-newlines": "^1.4.0",
    "eslint-plugin-jsdoc": "^48.2.3",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-unicorn": "^52.0.0",
    "eslint-plugin-unused-imports": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "typescript": "^5.4.5"
  }
}
